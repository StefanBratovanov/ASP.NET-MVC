@model  PhotoContest.App.Areas.Admin.Models.HomeAdminViewModel

<h1 style="color: red">Admin Area</h1>

<div class="row">
    <div class="panel panel-default col-lg-4">
        <div class="panel-heading text-center">
            <h4>Edit Contests</h4>
        </div>
        <div class="panel-body ">
            @foreach (var contest in Model.Contests)
            {
                @Html.ActionLink(contest.Title, "EditContest", "ContestsAdmin",
                    new {area = "Admin", id = contest.Id}, new {@class = "btn btn-default" })
            }
        </div>
    </div>
</div>



